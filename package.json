{
  "name": "wellnesswave",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)",
      "/node_modules/@react-native-community/async-storage/(?!(lib))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "__test__/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "src/navigation/*.{js,jsx,ts,tsx}",
      "src/screens/*.{js,jsx,ts,tsx}",
      "src/screens/*/*.{js,jsx,ts,tsx}",
      "src/screens/*/*.{js,jsx,ts,tsx}",
      "src/components/*.{js,jsx,ts,tsx}",
      "src/utils/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/jestSetupFile.js"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "react-native-community/datetimepicker",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.16",
    "@types/react-dom": "^18.2.18",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.7",
    "crypto": "^1.0.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "expo": "~50.0.3",
    "expo-constants": "^15.4.5",
    "expo-status-bar": "~1.11.1",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.1",
    "jest-styled-components": "7.0.0-beta.1",
    "mock-async-storage": "^2.2.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.2",
    "react-native-dotenv": "^3.4.9",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-pager-view": "^6.2.3",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-tab-view": "^3.5.2",
    "react-test-renderer": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-dom": "^6.3.0",
    "@types/react": "18.2.48",
    "typescript": "^5.1.3"
  },
  "private": true
}
